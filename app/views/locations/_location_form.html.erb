<div class="locationForm">
<%= form_for @location, :html => {:class => "submitOrganizationLocation"} do |f| %>
  <p><%= f.label :title %> <%= f.text_field :title %></p>
  <p><%= f.label :organization_name %> <%= f.text_field :organization_name %></p>
  <p><%= f.label :address_street %> <%= f.text_field :address_street %><br />
  <p><%= f.label :city %> <%= f.text_field :city %> <%= f.label :state %> <%= f.text_field :state %> <%= f.label :zipcode %> <%= f.text_field :zipcode %></p>
  <p><%= f.label :description %> <%= f.text_area :description %></p>
  <p><%= f.label :website %> <%= f.url_field :website %></p>
  <p><%= f.label :twitter_handle %> <%= f.text_field :twitter_handle %></p>
  <p><%= f.submit :class => "submitOrganizationLocation" %></p>
<% end %>
</div>